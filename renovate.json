{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "uv"
      ],
      "automerge": true
    },
    {
      "matchPackageNames": [
        "serieux",
        "gifnoc"
      ],
      "groupName": "breuleux"
    }
  ]
}